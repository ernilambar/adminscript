{
	"name": "adminscript",
	"version": "1.0.1",
	"description": "Adminscript",
	"author": "Nilambar Sharma",
	"license": "GPL-2.0-or-later",
	"main_file": "adminscript.php",
	"keywords": [
		"wordpress",
		"wordpress-plugin"
	],
	"homepage": "https://github.com/ernilambar/adminscript",
	"repository": {
		"type": "git",
		"url": "https://github.com/ernilambar/adminscript.git"
	},
	"bugs": {
		"url": "https://github.com/ernilambar/adminscript/issues"
	},
	"browserslist": "> 1%, not dead",
	"packtor": {
		"files": [
			"**/*",
			"!*.js",
			"!*.json",
			"!*.lock",
			"!*.md",
			"!*.yaml",
			"!languages/**/*",
			"!src/**/*"
		]
	},
	"devDependencies": {
		"@babel/preset-env": "^7.24.8",
		"@wordpress/prettier-config": "^4.3.0",
		"babel-loader": "^9.1.3",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.0",
		"easy-replace-in-files": "^1.0.3",
		"mini-css-extract-plugin": "^2.9.0",
		"packtor": "^1.0.2",
		"postcss": "^8.4.39",
		"postcss-loader": "^8.1.1",
		"postcss-nested": "^6.0.1",
		"postcss-preset-env": "^9.6.0",
		"prettier": "npm:wp-prettier@^3.0.3",
		"rimraf": "^6.0.0",
		"webpack": "^5.93.0",
		"webpack-cli": "^5.1.4"
	},
	"scripts": {
		"dev": "webpack --watch",
		"build": "webpack --mode=production --no-devtool",
		"format": "prettier \"**/*.{cjs,css,js,json,mjs}\" --write",
		"version": "easy-replace-in-files",
		"pot": "composer run-script pot",
		"clean": "rimraf build deploy vendor",
		"vendor": "composer install --no-dev --no-scripts -o",
		"predeploy": "pnpm run clean && pnpm run vendor && pnpm run build",
		"deploy": "packtor"
	}
}
